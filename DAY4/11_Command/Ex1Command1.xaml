<Window x:Class="_11_Command.Ex1Command1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_11_Command"
        mc:Ignorable="d"
        Title="Ex1Command1" Height="450" Width="800">
    <StackPanel>
        <TextBox x:Name="txtbox" Width="300" Height="50" Margin="10" TextChanged="txtbox_TextChanged"/>
        <CheckBox Checked="checkbox_Checked" x:Name="checkbox" Content="동의" HorizontalAlignment="Center" Margin="10"/>
        <Button Click="button_Click" IsEnabled ="False" x:Name="button" Content="로그인" FontSize="20" Margin="10"/>
    </StackPanel>
    
    
    
    
</Window>
